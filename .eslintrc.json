{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "double"],
    "semi": [2, "always"]
  }
}
